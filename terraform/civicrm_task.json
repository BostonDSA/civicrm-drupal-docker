[
    {
	"name": "${project_name}",
	"image": "${aws_account_id}.dkr.ecr.${aws_region}.amazonaws.com/${project_name}:latest",
	"memory": 512,
	"cpu": 0,
	"portMappings": [
	    {
		"containerPort": 80,
		"hostPort": 80
	    }
	],
	"environment": [
	    {
		"name": "MYSQL_DATABASE",
		"value": "${org_name}"
	    },
	    {
		"name": "MYSQL_USER",
		"value": "${org_name}"
	    },
	    {
		"name": "MYSQL_PASSWORD",
		"value": "${mysql_password}"
	    },
	    {
		"name": "MYSQL_HOST",
		"value": "${mysql_host}"
	    },
	    {
		"name": "MYSQL_PORT",
		"value": "3306"
	    },
	    {
		"name": "DRUPAL_HASH_SALT",
		"value": "${drupal_hash_salt}"
	    }
	]
    }
]
